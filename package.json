{
  "name": "seonest-be",
  "description": "seonest.net back-end",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/JHSeo-git/seonest-be.git"
  },
  "author": {
    "name": "JHSeo",
    "email": "qnfqnfqnf@gmail.com"
  },
  "keywords": [
    "nodejs",
    "koajs",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:sync": "yarn typeorm schema:sync",
    "deploy": "serverless deploy",
    "deploy:skip": "serverless deploy --skip-package",
    "deploy:prod": "serverless deploy --stage prod",
    "build": "tsc",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:setup": "cat ./ecosystem.config.js && pm2 deploy ecosystem.config.js production setup",
    "pm2:deploy": "cat ./ecosystem.config.js && pm2 deploy ecosystem.config.js production --force"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.75",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-logger": "^3.1.1",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "^8.0.4",
    "@types/mime-types": "^2.1.0",
    "@types/mysql": "^2.15.18",
    "@types/node": "^14.14.34",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "aws-sdk": "^2.884.0",
    "dotenv": "^8.2.0",
    "googleapis": "^67.1.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-logger": "^3.2.1",
    "markdown-to-text": "^0.1.1",
    "mime-types": "^2.1.30",
    "mysql": "^2.18.1",
    "pm2": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^2.7.0",
    "serverless-offline": "^6.9.0",
    "serverless-webpack": "^5.4.1",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.31",
    "typescript": "4.4.4",
    "webpack": "^5.33.2",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "serverless-dotenv-plugin": "^3.9.0"
  }
}
